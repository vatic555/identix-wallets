    "nestjs-pino-logger": "^0.1.0",
    "nestjs-typeorm-paginate": "^3.1.3",
    "newman": "^5.2.3",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
