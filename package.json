    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.3",
    "apollo-server-express": "^3.6.7",
    "cache-manager": "^3.4.0",
    "compression": "^1.7.4",
